{
	"name": "Listen Up",
	"description": "A alarm clock to play music every 1 hour to get a rest from staring at screen.",
	"version": "1.0",
	"manifest_version":2,
	"author": "Lily Zhang",
	"permissions": ["alarms", "notifications", "storage"],
	"background":{
		"scripts":["background.js"],
		"persistent":false
	},
	"browser_action":{
		"default_title": "Listen to some fresh music",
		"default_popup": "popup.html"
	},
	"icons":{
		"16": "music_tempo.jpg"
	}


}